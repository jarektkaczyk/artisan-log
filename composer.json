{
    "name": "sofa/laravel-artisan-log",
    "description": "Logging for Laravel artisan commands and scheduled tasks",
    "type": "library",
    "require": {
        "php": ">=7.4",
        "illuminate/console": ">=7.0",
        "illuminate/support": ">=7.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "phpstan/phpstan": "^0.12",
        "mockery/mockery": "^1.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jarek Tkaczyk",
            "email": "jarek@softonsofa.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Sofa\\ArtisanLog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sofa\\ArtisanLog\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Sofa\\ArtisanLog\\ArtisanLogServiceProvider"
            ]
        }
    }
}
